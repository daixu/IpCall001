<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="wrap_content" android:layout_width="fill_parent"
	android:padding= "10dip">
	<ImageView android:src="@drawable/icon" android:id="@+id/imageContactPhoto"
		
		android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:orientation="vertical">
		<TextView android:layout_gravity="center_vertical" android:textSize="50dip" android:layout_width="wrap_content" android:text="text" android:id="@+id/textContactName" android:layout_height="wrap_content"></TextView>
	</LinearLayout>
</LinearLayout>